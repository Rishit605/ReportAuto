             xmlns:local="clr-namespace:ReportAuto"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Theme/MenuButtonTheme.xaml"/>
                <ResourceDictionary Source="Theme/EmailTextBoxTheme.xaml"/>
                <ResourceDictionary Source="Theme/DateTextBoxTheme.xaml"/>
                <ResourceDictionary Source="Theme/WDInputTextBoxTheme.xaml"/>
                <ResourceDictionary Source="Theme/AchieveInputTextBoxTheme.xaml"/>
                <ResourceDictionary Source="Theme/EmailSubjectTextBoxTheme.xaml"/>
                <ResourceDictionary Source="Theme/Brushes.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
